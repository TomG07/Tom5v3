{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "rootDir": "ts",
    "outDir": "js",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "pretty": false,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "public"],
}